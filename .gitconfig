[core]
	autocrlf = input
	safecrlf = false
	editor = pico

[alias]
    resetorigin = !git fetch origin && git reset --hard origin/master && git clean -f -d
    resetupstream = !git fetch upstream && git reset --hard upstream/master && git clean -f -d
    ls = !git ls-tree -r master --name-only

